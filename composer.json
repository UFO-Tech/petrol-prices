    "description": "Library to fetch and cache petrol prices from public resources.",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ufo\\PetrolPrices\\": "src/",
            "Tests\\Ufo\\PetrolPrices\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alex Maystrenko",
            "email": "ashterix69@gmail.com"
        }
    ],
    "require": {
